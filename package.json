{
  "name": "ethlog",
  "version": "0.12.2",
  "description": "Best effort for retrive logs from ETH-like JSON RPC endpoints",
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "mocha --exit --recursive --timeout 10000",
    "standardize": "palmito -i '**/docs/**' -i '**/*.tgz' -i '**/tmp/**' -i '**/doc/**'",
    "doc": "jsdoc --destination docs --configure doc/jsdoc.json lib doc/jsdoc_home.md",
    "doc-server": "http-server docs",
    "doc-clean": "rm -rf docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevin-leptons/ethlog.git"
  },
  "keywords": [
    "eth",
    "log",
    "json_rpc"
  ],
  "author": "Kevin Leptons",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevin-leptons/ethlog/issues"
  },
  "homepage": "https://github.com/kevin-leptons/ethlog#readme",
  "devDependencies": {
    "axios-mock-adapter": "1.20.0",
    "docdash": "1.2.0",
    "http-server": "14.0.0",
    "jsdoc": "3.6.7",
    "mocha": "9.2.0",
    "mock-fs": "5.1.2",
    "mockdate": "3.0.5",
    "palmito": "0.4.0"
  },
  "dependencies": {
    "@ethersproject/abi": "5.6.0",
    "@ethersproject/bignumber": "5.6.0",
    "@trop/gear": "0.4.2",
    "axios": "0.24.0",
    "minitype": "0.16.1",
    "stdio_log": "0.10.0"
  }
}
